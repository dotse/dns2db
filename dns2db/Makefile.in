PROG=dns2db
PCAP_READER=@PCAP_READER@
CC=@CC@
LIB=@LIBS@
LDFLAGS=@LDFLAGS@ 
LIBOBJS=@LIBOBJS@
CFLAGS= @CFLAGS@ @CPPFLAGS@ @DEFS@


OBJS=dns2db.o \
	$(PCAP_READER) \
	arg_handle.o \
	tcp_factory_assemble.o \
	e_filter.o 




all: $(PROG)



$(PROG): $(OBJS)
	$(CXX) -o $@ $(OBJS) $(LDFLAGS) $(LIBOBJ) $(LIB)


clean:
	rm -rf $(OBJS)
	rm -rf $(PROG)

clean_temp:
	rm -rf *~
